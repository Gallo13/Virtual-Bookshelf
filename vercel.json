{
    "version": 2,
    "builds": [
        {
            "src": "Virtual-Bookshelf.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Virtual-Bookshelf.py"
        }
    ],
    "env": {
        "DATABASE_URL": "mysql://username:password@host:port/dbname"
    }
}